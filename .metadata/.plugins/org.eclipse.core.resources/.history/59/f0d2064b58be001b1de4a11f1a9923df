package repeticao;

public class ExemploFor {
	
	public static void main(String[] args) {
		
		/*
		 * Sintaxe do for:
		 * 
		 * 1- variavel que vai contas
		 * 2- condição
		 * 3- como ele vai contar?
		 * 
		 */
		
		for (int contador = 0; contador < 50; contador+=2 ){
			System.out.println(contador);
			
		}
		
	}

}
